{
  "name": "react-animated-js",
  "version": "1.3.1",
  "description": "React component to show or hide elements with animations using [Animated.css](https://daneden.github.io/animate.css/)",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs --css-modules false",
    "start": "microbundle-crl watch --no-compress --format modern,cjs --css-modules false",
    "dist": "microbundle-crl --no-compress --format modern,cjs --css-modules false",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joaomiguelnt/react-animated-css.git"
  },
  "keywords": [
    "react",
    "animated",
    "animation",
    "animated.css"
  ],
  "author": "Fareed Alnamrouti <digital.flowers@hotmail.com>",
  "contributors": [
    "Niccolas Costa <niccolas.costa@gmail.com>",
    "Joao Noronha <joaomiguelnt@gmail.com>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaomiguelnt/react-animated-css/issues"
  },
  "homepage": "https://github.com/joaomiguelnt/react-animated-css#readme",
  "devDependencies": {
    "microbundle-crl": "0.13.11"
  },
  "dependencies": {
    "browser-or-node": "^1.0.2",
    "classnames": "^2.2.5",
    "lodash": "^4.17.11",
    "prop-types": "15.7.2",
    "react": "16.13.1"
  },
  "files": [
    "dist"
  ]
}
